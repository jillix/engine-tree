{
  "name": "engine-tree",
  "version": "1.0.0",
  "description": "Tree module for Engine.",
  "main": "server/index.js",
  "composition": {
    "public": "public",
    "flow": [
      {
        "on": "readDir",
        "call": "readDir"
      },
      {
        "on": "newFolder",
        "call": "newFolder"
      },
      {
        "on": "newFile",
        "call": "newFile"
      },
      {
        "on": "renamed",
        "call": "renamed"
      },
      {
        "on": "delete",
        "call": "delete"
      }
    ],
    "client": {
      "module": [
        "client/index.js",
        "client/libs/jstree.js",
        "/libs/jquery.js"
      ],
      "flow": [
        {
          "on": "_beforeSelectRes",
          "call": "_beforeSelectRes"
        },
        {
          "on": "nodeOpened",
          "call": "nodeOpened"
        },
        {
          "on": "open",
          "call": "open"
        }
      ],
      "styles": [
        "public/css/jstree.css"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jillix/engine-tree.git"
  },
  "keywords": [
    "jxengine",
    "tree"
  ],
  "author": "jillix <contact@jillix.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jillix/engine-tree/issues"
  },
  "homepage": "https://github.com/jillix/engine-tree",
  "dependencies": {
    "engine-tools": "^1.9.0",
    "rimraf": "^2.3.4",
    "mkdirp": "^0.5.1",
    "streamp": "^1.0.0"
  },
  "devDependencies": {}
}
