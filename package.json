{
  "name": "engine-tree",
  "version": "1.0.0",
  "description": "Tree module for Engine.",
  "main": "server/index.js",
  "public": "public",
  "components": {
    "scripts": [
      "client/index.js",
      "client/libs/jstree.js",
      "/libs/jquery.js"
    ],
    "styles": [
      "public/css/jstree.css"
    ]
  },
  "flow": [
    {
      "in": "readDir",
      "out": [
        {
          "call": "readDir"
        }
      ]
    },
    {
      "in": "newFolder",
      "out": [
        {
          "call": "newFolder"
        }
      ]
    },
    {
      "in": "newFile",
      "out": [
        {
          "call": "newFile"
        }
      ]
    },
    {
      "in": "rename",
      "out": [
        {
          "call": "rename"
        }
      ]
    },
    {
      "in": "delete",
      "out": [
        {
          "call": "delete"
        }
      ]
    }
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jillix/engine-tree.git"
  },
  "keywords": [
    "jxengine",
    "tree"
  ],
  "author": "jillix <contact@jillix.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jillix/engine-tree/issues"
  },
  "homepage": "https://github.com/jillix/engine-tree",
  "dependencies": {
    "engine-tools": "^1.9.0"
  },
  "devDependencies": {}
}
