{
  "name": "engine-tree",
  "version": "1.0.0",
  "description": "Tree module for Engine.",
  "main": "server/index.js",
  "composition": {
    "public": "public",
    "flow": [
      [
        "readDir",
        "readDir"
      ],
      [
        "newFolder",
        "newFolder"
      ],
      [
        "newFile",
        "newFile"
      ],
      [
        "renamed",
        "renamed"
      ],
      [
        "delete",
        "delete"
      ]
    ],
    "client": {
      "module": [
        "client/index.js",
        "client/libs/jstree.js",
        "/libs/jquery.js"
      ],
      "flow": [
        [
          "nodeOpened",
          ":nodeOpened"
        ],
        [
          "loaded",
          [
            ":ALTR",
            {
              "set": 
              {
                "path": "_path"
              }
            }
          ],
          ":openPath"
        ],
        [
          "readDir",
          ["flow", "@readDir"]
        ],
        [
          "newFolder",
          ["flow", "@newFolder"]
        ],
        [
          "newFile",
          ["flow", "@newFile"]
        ],
        [
          "renamed",
          ["flow", "@renamed"]
        ],
        [
          "delete",
          ["flow", "@delete"]
        ]
      ],
      "styles": [
        "public/css/jstree.css"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jillix/engine-tree.git"
  },
  "keywords": [
    "jxengine",
    "tree"
  ],
  "author": "jillix <contact@jillix.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jillix/engine-tree/issues"
  },
  "homepage": "https://github.com/jillix/engine-tree",
  "dependencies": {
    "engine-tools": "^1.9.0",
    "rimraf": "^2.3.4",
    "mkdirp": "^0.5.1",
    "streamp": "^1.0.0"
  },
  "devDependencies": {}
}
